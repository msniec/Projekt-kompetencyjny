<Page class="page"
    navigatingTo="onNavigatingTo" 
    xmlns="http://schemas.nativescript.org/tns.xsd">
    
    <ActionBar class="action-bar">
        <Label class="action-bar-title" text="Buttons Page"></Label>
    </ActionBar>
   
    
     <!-- <button text = "{{to co pobierzesz z fora}}" tap="onTab({{to co pobierasz z fora}})"> -->

    <ListView items="{{ items }}" class="list-group"> <!-- item -->
    <ListView.itemTemplate>
        <StackLayout class="list-group-item">
            <!-- <Label text="Date" class="list-group-item-heading" /> -->
            <!-- use $value to bind plain objects (e.g. number, string, Date)-->
            <Button text= "{{ $value.name }}" col="1" tap="onLabelTap" class="list-group-item-heading" style="{{$value.src}}"/>
            
        </StackLayout>
    </ListView.itemTemplate>
</ListView>
    
        
    
</Page>
